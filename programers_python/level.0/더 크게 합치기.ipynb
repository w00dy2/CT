    "def solution(a, b):\n",
    "    x = int(str(a) + str(b))\n",
    "    y = int(str(b) + str(a))\n",
    "    \n",
    "    if x > y:\n",
    "        answer = x\n",
    "    else:\n",
    "        answer = y\n",
    "\n",
    "    return answer"
   ]
